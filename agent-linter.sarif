{
  "version": "2.1.0",
  "$schema": "https://json.schemastore.org/sarif-2.1.0.json",
  "runs": [
    {
      "tool": {
        "driver": {
          "name": "agent-linter",
          "informationUri": "https://verifiablelabs.com",
          "rules": [
            {
              "id": "AL003",
              "name": "AL003",
              "shortDescription": {
                "text": "agent-linter rule AL003"
              }
            },
            {
              "id": "AL102",
              "name": "AL102",
              "shortDescription": {
                "text": "agent-linter rule AL102"
              }
            },
            {
              "id": "AL201",
              "name": "AL201",
              "shortDescription": {
                "text": "agent-linter rule AL201"
              }
            }
          ],
          "version": "dev"
        }
      },
      "results": [
        {
          "ruleId": "AL003",
          "level": "warning",
          "message": {
            "text": "action \"create_user\" parameter \"payload\" has overly permissive type \"any\""
          }
        },
        {
          "ruleId": "AL102",
          "level": "error",
          "message": {
            "text": "invocation of \"create_user\" missing required params: payload"
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "invocations.jsonl"
                }
              }
            }
          ]
        },
        {
          "ruleId": "AL201",
          "level": "error",
          "message": {
            "text": "action \"create_user\" wired to production target \"user-service\" by default"
          }
        }
      ]
    }
  ]
}
